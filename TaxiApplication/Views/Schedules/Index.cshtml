@model IEnumerable<TaxiApplication.Data.Schedule>

@using TaxiApplication.BusinessLogics
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Admin.cshtml";
    ScheduleLogics cl = new ScheduleLogics();
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.owner.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.owner.rank.RankName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.route.RouteName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ScheduleDate.ExtendSchedule)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Position)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.owner.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.owner.rank.RankName)
           
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.route.RouteName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ScheduleDate.ExtendSchedule)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Position)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.No }) |
                @Html.ActionLink("Details", "Details", new { id = item.No }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.No })
            </td>
        </tr>
    }

</table>
